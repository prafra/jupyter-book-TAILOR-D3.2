abstract = "Two oddity tasks were compared: one in which the odd stimulus could appear on any of the three stimulus positions (true oddity); the other in which the odd stimulus could only appear on the left or right but not the center (oddity-from-sample). A stimulus-configuration theory predicts faster oddity-from-sample learning, whereas a theory, based on shifts in observing over trials, predicts faster learning of true oddity. Pigeons learned the oddity-from-sample task faster, thus supporting configuration theory. Performance on the oddity-from-sample task but not the true oddity task was facilitated by increasing the number of responses required to terminate a trial. When the oddity-from-sample pigeons were shifted to the true oddity task, no evidence of positive transfer was found, suggesting that a configuration theory alone is insufficient to explain differences in acquisition. Only when a correction procedure was introduced did the true oddity pigeons perform above 50%. "
}



@INPROCEEDINGS{lovett12analogies,
     author = {Lovett, A. and Forbus, K.},
   keywords = {Geometric analogy, Structure-mapping, visual problem-solving},
      title = {Modeling multiple strategies for solving geometric analogy problems},
  booktitle = {Proceedings of the 34th Annual Conference of the Cognitive Science Society},
       year = {2012},
      pages = {701-706},
   location = {Sapporo, Japan},
       note = {Cogsci 2012 Proceedings site: <a href="http://mindmodeling.org/cogsci2012/index.html">http://mindmodeling.org/cogsci2012/index.html</a>},
       isbn = {978-0-9768318-8-4},
        url = {http://www.qrg.northwestern.edu/papers/Files/QRG_Dist_Files/QRG_2012/Lovett_CogSci12_GeoAnalogy_Final.pdf},
   abstract = {We present an improved computational model for performing geometric analogy. The model combines two previously modeled strategies and makes explicit claims about when people will use one strategy or the other. We compare the model to human performance on a classic problem set. The model’s strategy shifts, along with working memory load, account for most of the variance in human reaction times.}
}


@INPROCEEDINGS{lovett05analogies,
     author = {Tomai, E. and Lovett, A. and Forbus, K. and Usher, J.},
      title = {A Structure Mapping Model for Solving Geometric Analogy Problems},
  booktitle = {Proceedings of the 27th Annual Conference of the Cognitive Science Society},
       year = {2005},
      pages = {2190-2195},
  publisher = {Cognitive Science Society, Inc},
    address = {Stressa, Italy},
        url = {http://www.qrg.northwestern.edu/papers/Files/f831-tomai.pdf},
   abstract = {Evan's 1968 ANALOGY system was the first
computer model of analogy. This paper
demonstrates that structure-mapping, when
combined with high-level visual processing and
qualitative representations, can do the same kinds
of problems with hand-drawn sketched inputs.
Importantly, the bulk of the computations are not
particular to the model of this task but are general
purpose: we sketch the problems using our
existing sketch understanding system, sKEA,
which computes visual structure that is used by our
existing analogical matcher, SME. We show how
SME can be used to facilitate high-level visual
matching and how second-order analogies over
differences computed via analogies between
sketches provide a more elegant model of this task.}
}


